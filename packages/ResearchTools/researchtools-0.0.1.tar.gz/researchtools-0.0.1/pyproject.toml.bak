[build-system]
requires = ["setuptools", "setuptools-scm"]
build-backend = "setuptools.build_meta"

[project]
name='ResearchTools'
version='1.0'
dependencies = ['numpy>1.10', 'psutil', 'scipy>=0.17.0', 'numba>=0.54',
'dill @ git+ssh://git@github.com/uqfoundation/dill.git@master',
'pathos @ git+ssh://git@github.com/uqfoundation/pathos.git@master'
]
